{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"maxPoints": 1,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"gameScene": {
			"type": "script",
			"path": "script/scenes/gameScene.js",
			"global": true
		},
		"character": {
			"type": "script",
			"path": "script/entities/character.js",
			"global": true
		},
		"timer": {
			"type": "script",
			"path": "script/entities/timer.js",
			"global": true
		},
		"female": {
			"type": "image",
			"path": "image/female.png",
			"width": 32,
			"height": 32
		},
		"male": {
			"type": "image",
			"path": "image/male.png",
			"width": 32,
			"height": 32
		},
		"score": {
			"type": "script",
			"path": "script/entities/score.js",
			"global": true
		},
		"scoreHandler": {
			"type": "script",
			"path": "script/scoreHandler.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"ranking"
			],
			"preferredSessionParameters": {
				"totalTimeLimit": 35
			}
		}
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-timeline/lib/ActionType.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Easing.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Timeline.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/Tween.js",
		"node_modules/@akashic-extension/akashic-timeline/lib/index.js"
	],
	"moduleMainScripts": {
		"@akashic-extension/akashic-timeline": "node_modules/@akashic-extension/akashic-timeline/lib/index.js"
	}
}